    "babel-preset-react-native": "^4.0.1",
    "expo": "~47.0.8",
    "expo-constants": "^14.0.2",
    "expo-permissions": "^14.0.0",
    "expo-status-bar": "~1.4.2",
    "interopRequireDefault": "link:@babel/runtime/helpers/interopRequireDefault",
    "react": "18.1.0",
    "react-dom": "18.1.0",
